#include "fsm_automatic.h"

void fsm_automatic_run(){
	switch(status){
	case INIT:
		//TODO
		HAL_GPIO_WritePin(LED_RED_GPIO_Port, LED_RED_Pin, PinState);
		status = AUTO_RED;
		setTimer(500)
		break;
	case AUTO_RED:
		break;
	case AUTO_YELLOW:
		break;
	case AUTO_GREEN:
		break;
	default:
		break;
	}
}
